[server]
# This enables the app to serve local files (like fonts) publicly
enableStaticServing = true 

# Define your custom font using Streamlit's fontFaces table
# The 'url' path MUST start with 'app/static/'
[[theme.fontFaces]]
family = "ImpactMeme" # Choose a simple name for your font
url = "app/static/Impact.ttf"
style = "normal"
weight = 400
